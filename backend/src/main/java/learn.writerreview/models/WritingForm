import java.lang.*;
public class WritingForm {
    private int genreId;
    private String genreName;

    public WritingForm(int genreId, String genreName) {
        this.genreId = genreId;
        this.genreName = genreName;
    }

    public int getGenreId() {
        return genreId;
    }

    public void setGenreId(int genreId) {
        this.genreId = genreId;
    }

    public String getGenreName() {
        return genreName;
    }

    public void setGenreName(String genreName) {
        this.genreName = genreName;
    }

    public boolean equals(Object object) {
        if (this == object) return true;
        if (object == null || getClass() != object.getClass()) return false;
        if (!super.equals(object)) return false;
        WritingForm that = (WritingForm) object;
        return genreId == that.genreId && java.util.Objects.equals(genreName, that.genreName);
    }

    public int hashCode() {
        return Objects.hash(super.hashCode(), genreId, genreName);
    }

    @Override
    public java.lang.String toString() {
        return "WritingForm{" +
                "genreId=" + genreId +
                ", genreName='" + genreName + '\'' +
                '}';
    }
}
